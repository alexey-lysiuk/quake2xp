attribute vec2		a_texCoord;
attribute vec2		a_2texCoord;
attribute vec4		a_colorArray;
attribute vec3		a_vertArray;

varying vec2		v_texCoord;
varying vec2		v_texCoord1;
varying vec4		v_texCoordSky;
varying vec4		v_colorArray;

uniform int			u_isSky;
uniform mat4		u_SkyMatrix;

void main (void) 
{
	v_texCoord		= a_texCoord;
	v_texCoord1		= a_2texCoord; 
	v_colorArray	= a_colorArray;

	gl_Position		= gl_ModelViewProjectionMatrix * vec4(a_vertArray, 1.0);

}
